<Questionnaire xmlns="http://hl7.org/fhir">
	<meta>
		<profile value="http://highmed.org/fhir/StructureDefinition/questionnaire|0.7.0" />
		<tag>
			<system value="http://highmed.org/fhir/CodeSystem/read-access-tag" />
			<code value="ALL" />
		</tag>
	</meta>
	<url value="http://highmed.org/fhir/Questionnaire/hello-world"/>
	<!-- version managed by bpe -->
	<version value="#{version}"/>
	<!-- date managed by bpe -->
	<date value="#{date}"/>
	<!-- status managed by bpe -->
	<status value="unknown"/>
	<item>
		<linkId value="business-key"/>
		<type value="string"/>
		<text value="The business-key of the process execution"/>
	</item>
	<item>
		<linkId value="user-task-id"/>
		<type value="string"/>
		<text value="The user-task-id of the process execution"/>
	</item>
	<item>
		<linkId value="data-set-location"/>
		<type value="string"/>
		<text value="Add the location (url) of the data-set that should be approved"/>
	</item>
	<item>
		<linkId value="approve"/>
		<type value="boolean"/>
		<text value="Do you approve the release of the data-set?"/>
	</item>
</Questionnaire>